%define DTED_NONE 0
%define DTED_SRTM 1

%define DTED_ERROR_NONE   0
%define DTED_ERROR_FILE   1
%define DTED_ERROR_MEMORY 2
%define DTED_ERROR_DATA   3

%define DTED_TOTALPX 1201
%define DTED_RTCM_SIZE (DTED_TOTALPX*DTED_TOTALPX*2)
%define DTED_SECONDSPERPIX (3600/(DTED_TOTALPX-1))

%Dir dted_ref {
  char source = DTED_NONE       :0=none, 1=SRTM;
  char error  = DTED_ERROR_NONE :0=none, 1=no file, 2=no memory, 3=bad data;
  double elevation = 0          :(ft) terrain elevation above datum;
  double manualShift = 0        :(ft) to allow manual adjustment (normally zero);
  char checkMan = 0             :(bool) to enable checking elevation at lat, lon manually;
  double manLat = 0             :(deg) check manual;
  double manLon = 0             :(deg) check manual;
  double manEle = 0             :(ft) terrain elevation above datum;
  int latDec = 255              :(deg);
  int lonDec = 255              :(deg);
  voidptr fd = NULL             :file descriptor;
  voidptr srtmTile = NULL       :pointer to data in memory;
  string filename = "\0                                                                                                                                      "  :file name for dted;
} dted, sceneDTED;



